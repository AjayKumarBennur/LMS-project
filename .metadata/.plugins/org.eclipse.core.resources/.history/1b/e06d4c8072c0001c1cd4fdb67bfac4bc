package com.ajay.lms;

import java.util.ArrayList;
import java.util.Arrays;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;

import com.ajay.lms.pojo.BatchDetails;
import com.ajay.lms.pojo.Technologies;

@SpringBootApplication
public class LmsApplication {

	public static <E> void main(String[] args) {
		SpringApplication.run(LmsApplication.class, args);
		
		BatchDetails batchDetails = new BatchDetails();
		
		ArrayList<Technologies> arrayList = new ArrayList<Technologies>();
		arrayList.add(Technologies.JAVA);
		batchDetails.setTechnologies(arrayList);
		System.out.println(batchDetails.getTechnologies());
	}
	
		
	

}
